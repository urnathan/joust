RUN: ezio -i $srcdir/$src $src |
RUN: ezio -p COM -p OUT $src | ezio -p COM -p ERR $src

BLOCK checking

one
two
three
three
four
four
five
five

CHECK-BLOCK: two
CHECK-BLOCK: three
CHECK-BLOCK: four
CHECK: four
CHECK-BLOCK: five
CHECK-NEXT: five

ERR: {:[0-9]+}:one
ERR-NEXT: {:[0-9]+}:two
COM: PASS: 03-ezio/block-1:15:BLOCK  two
ERR-NEXT: {:[0-9]+}:three
COM-NEXT: PASS: 03-ezio/block-1:16:BLOCK  three
ERR-NEXT: {:[0-9]+}:three
ERR-NEXT: {:[0-9]+}:four
COM-NEXT: PASS: 03-ezio/block-1:17:BLOCK  four
ERR-NEXT: {:[0-9]+}:four
COM-NEXT: PASS: 03-ezio/block-1:18:MATCH  four
ERR-NEXT: {:[0-9]+}:five
COM-NEXT: PASS: 03-ezio/block-1:19:BLOCK  five
ERR-NEXT: {:[0-9]+}:five
COM-NEXT: PASS: 03-ezio/block-1:20:NEXT  five
OUT-NEXT: $EOF
