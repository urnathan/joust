RUN: ezio -i $srcdir/$src $src |
RUN: ezio -p COM -p OUT $src | ezio -p COM -p ERR $src

LABEL checking

one
two
three
three
four
four
five
five

CHECK-LABEL: two
CHECK-LABEL: three
CHECK-LABEL: four
CHECK: four
CHECK-LABEL: five
CHECK-NEXT: five

ERR: {:[0-9]+}:one
ERR-NEXT: {:[0-9]+}:two
COM: PASS: 03-ezio/label-1:15:LABEL  two
ERR-NEXT: {:[0-9]+}:three
COM-NEXT: PASS: 03-ezio/label-1:16:LABEL  three
ERR-NEXT: {:[0-9]+}:three
ERR-NEXT: {:[0-9]+}:four
COM-NEXT: PASS: 03-ezio/label-1:17:LABEL  four
ERR-NEXT: {:[0-9]+}:four
COM-NEXT: PASS: 03-ezio/label-1:18:MATCH  four
ERR-NEXT: {:[0-9]+}:five
COM-NEXT: PASS: 03-ezio/label-1:19:LABEL  five
ERR-NEXT: {:[0-9]+}:five
COM-NEXT: PASS: 03-ezio/label-1:20:NEXT  five
OUT-NEXT: $EOF
